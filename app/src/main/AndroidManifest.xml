<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yunbu.sdk.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.yunbu.sdk.demo.GameActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="YUNBU_KEY"
            android:value="YUNBU_DEMO_KEY_123" />
        <meta-data
            android:name="YUNBU_DEBUG"
            android:value="true" />

        <activity android:name=".LevelDetailActivity"></activity>
    </application>
</manifest>